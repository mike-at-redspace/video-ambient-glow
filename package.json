{
  "name": "video-ambient-glow",
  "version": "0.1.0",
  "description": "Lightweight library for adding YouTube-style ambient glow effects to HTML5 video elements",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:example": "npm run build && cd example && npm install && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "npm run format && eslint src tests example",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"example/**/*.{js,css,html}\" \"example/**/*.hbs\" \"example/*.{js,json}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"example/**/*.{js,css,html}\" \"example/**/*.hbs\" \"example/*.{js,json}\" \"*.{js,json,md}\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "video",
    "ambient",
    "glow",
    "effect",
    "canvas",
    "html5",
    "youtube",
    "backdrop",
    "video-effects",
    "visual-effects",
    "ui-effects"
  ],
  "author": "Mike Vardy <https://github.com/mike-at-redspace>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
