  "repository": {
    "type": "git",
    "url": "git+https://github.com/mike-at-redspace/video-ambient-glow.git"
  },
  "bugs": {
    "url": "https://github.com/mike-at-redspace/video-ambient-glow/issues"
  },
  "homepage": "https://github.com/mike-at-redspace/video-ambient-glow#readme",
